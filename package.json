{
  "name": "woa-getter",
  "version": "0.1.0",
  "author": "Moving_J",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "createHash": "electron ./build/createHash.js",
    "genFileToken": "electron ./build/genFileToken.js",
    "electron:build": "npm run createHash && npm run genFileToken && vue-cli-service electron:build",
    "electron:build-unpack": "npm run createHash && npm run genFileToken && vue-cli-service electron:build --dir",
    "electron:serve": "npm run createHash && npm run genFileToken && vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "electron-log": "^4.4.1",
    "element-ui": "^2.15.3",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "bytenode": "^1.3.3",
    "electron": "^12.0.0",
    "electron-builder": "22.10.5",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-obfuscator": "2.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 Chrome versions",
    "not dead"
  ]
}
